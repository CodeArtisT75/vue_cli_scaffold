{
  "name": "vue_cli_scaffold",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev": "concurrently \"npm run serve\" \"sass --watch ./src/main.scss ./public/main.css\"",
    "create": "sass ./src/main.scss:./public/main.css --style compressed && npm run build ",
    "css": "sass --watch ./src/main.scss ./public/main.css"
  },
  "dependencies": {
    "bootstrap-vue": "^2.15.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "concurrently": "^5.2.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
